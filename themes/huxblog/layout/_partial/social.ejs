<% if (config.github_username) { %>
<li>
    <a target="_blank"
       href="https://github.com/<%= config.github_username %>"
       data-toggle="tooltip" data-placement="top" title="<%= config.github_username %>">
        <span class="fa-stack fa-lg">
            <i class="fa fa-circle fa-stack-2x"></i>
            <i class="fa fa-github fa-stack-1x fa-inverse"></i>
        </span>
    </a>
</li>
<% } %>
<% if (config.weibo_username) { %>
<li>
    <a target="_blank"
       href="http://weibo.com/<%= config.weibo_username %>"
       data-toggle="tooltip" data-placement="top" title="<%= config.weibo_username %>">
        <span class="fa-stack fa-lg">
            <i class="fa fa-circle fa-stack-2x"></i>
            <i class="fa fa-weibo fa-stack-1x fa-inverse"></i>
        </span>
    </a>
</li>
<% } %>
<% if (config.email_name) { %>
<li>
    <a target="_blank"
       href="mailto:<%= config.email %>"
       data-toggle="tooltip" data-placement="top" title="<%= config.email %>">
        <span class="fa-stack fa-lg">
            <i class="fa fa-circle fa-stack-2x"></i>
            <i class="fa fa-envelope fa-stack-1x fa-inverse"></i>
        </span>
    </a>
</li>
<% } %>
<% if (config.weichat) { %>
<li>
    <a href="javascript:void(0)"
       data-toggle="tooltip" data-placement="top" title="<%= config.weichat %>">
        <span class="fa-stack fa-lg">
            <i class="fa fa-circle fa-stack-2x"></i>
            <i class="fa fa-weixin fa-stack-1x fa-inverse"></i>
        </span>
    </a>
</li>
<% } %>